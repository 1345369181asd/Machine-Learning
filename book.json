{
    "title":"机器学习",
    "author": "shunliz",
    "plugins": ["katex", 
                "image-captions",
                "github-buttons",
                "page-treeview"
    ],
  "pluginsConfig": {
    "image-captions": {
      "attributes": {
        "width": "600"
      },
